{
  "name": "nodetses6babel",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/app.js",
  "scripts": {
    "build": "babel src --extensions \".ts,.js\" --out-dir dist/src --copy-files",
    "ci": "pnpm run cpy:ci && pnpm run ts:check && pnpm run lint",
    "clean": "rimraf dist/src/*",
    "cpy:ci": "cp .env.dev .env",
    "start": "node dist/src/app.js",
    "start:dev": "nodemon",
    "lint": "eslint -c .eslintrc.js \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint -c .eslintrc.js \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pre:commit": "lint-staged",
    "pre:check": "pnpm ts:check && pnpm lint",
    "prepare": "husky install",
    "test": "jest",
    "test:cov": "jest --coverage",
    "ts:check": "tsc -p tsconfig.build.json --noEmit",
    "test:watch": "jest --watch --silent --coverage=false",
    "test:ci": "jest --run-in-band --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RoxnnyABarriosC/nodeTsEs6Babel.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RoxnnyABarriosC/nodeTsEs6Babel/issues"
  },
  "homepage": "https://github.com/RoxnnyABarriosC/nodeTsEs6Babel#readme",
  "dependencies": {
    "@digichanges/shared-experience": "^0.8.0",
    "awilix": "^8.0.0",
    "awilix-express": "^8.0.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-throttle": "^2.0.0",
    "helmet": "^6.0.1",
    "hpropagate": "^1.0.1",
    "i18n": "^0.15.1",
    "ini": "^3.0.1",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "pg-promise": "^11.2.0",
    "promise-events": "^0.2.4",
    "reflect-metadata": "^0.1.13",
    "ts-mixer": "^6.0.3",
    "tslog": "^4.7.2",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-decorators": "^7.19.0",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/plugin-transform-typescript": "^7.20.13",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.13",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/ini": "^1.3.31",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.33.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^8.0.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-environment-node": "^28.1.3",
    "jest-mock-extended": "^2.0.7",
    "lint-staged": "^13.1.1",
    "nodemon": "^2.0.20",
    "pg-mem": "^2.6.4",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.1",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>"
    ],
    "verbose": true,
    "testTimeout": 16000
  },
  "engines": {
    "node": ">=16.*"
  }
}
